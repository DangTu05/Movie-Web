{
  "name": "movie-web",
  "version": "1.0.0",
  "description": "",
  "main": "server/src/server.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon --exec ts-node server/src/server.ts",
    "check:eslint": "eslint . --config eslint.config.mjs --ext .ts,.js",
    "lint": "npm run check:eslint && npm run check:prettier",
    "check:prettier": "prettier --check \"**/*.{ts,json,md}\"",
    "format": "prettier --write \"**/*.{ts,json,md}\"",
    "check": "npm run lint && npm run check:eslint && npm run check:prettier",
    "copy-views": "cp -r server/src/views server/dist/views",
    "build:prettier": "prettier --write \"**/*.{ts,json,md}\"",
    "build:lint": "npm run build:eslint && npm run build:prettier",
    "build": "rimraf server/dist && tsc --project server/tsconfig.json && npm run copy-views",
    "ts.check": "tsc --project server/tsconfig.json",
    "dev": "cross-env BUILD_MODE=dev nodemon",
    "production": "npm run ts.check && npm run build && cross-env BUILD_MODE=prod node server/dist/server.js"
  },
  "pre-commit": [
    "ts.check",
    "build"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/multer": "^1.4.13",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "cloudinary": "^2.6.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "http-status-codes": "^2.3.0",
    "mongoose": "^8.15.2",
    "multer": "^2.0.1",
    "multer-storage-cloudinary": "^4.0.0",
    "node-cron": "^4.1.1",
    "pug": "^3.0.3",
    "winston": "^3.17.0",
    "zod": "^3.25.64"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/async-exit-hook": "^2.0.2",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^24.0.1",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "globals": "^16.2.0",
    "nodemon": "^3.1.10",
    "pre-commit": "^1.2.2",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.0"
  }
}
